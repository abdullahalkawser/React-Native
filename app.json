{
  "expo": {
    "name": "splash-screen",
    "slug": "splash-screen",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "native",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ff0000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#ff0000"
        }
      ]
    ],
    "splash": {
   
      "resizeMode": "contain",
      "backgroundColor": "#ff0000"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
